package test;

import java.util.ArrayList;

public class Test5 {

	public static void main(String[] args) {
//		Board 모든 값을 받는 생성자 추가
//		Board 모든 내용 출력하는 prn() 메서드 추가
//		Board 객체생성 게시판글 3개 준비
		Board board1 = new Board(1, "호길동", "제목1", "내용1");
		Board board2 = new Board(2, "이길동", "제목2", "내용2");
		Board board3 = new Board(3, "삼길동", "제목3", "내용3");
		
//		Board형을 담은 ArrayList배열 객체생성
		ArrayList<Board> arr = new ArrayList<Board>();
//		게시판글 3개 배열에 저장
		arr.add(board1);
		arr.add(board2);
		arr.add(board3);
		
//		for 출력
		for(int i = 0; i < arr.size(); i++) {
			System.out.println(arr.get(i));
			arr.get(i).prn();
		}
		
		for(Board b : arr) {
			b.prn();
		}
		
		
//		BoardDAO 객체생성
//		리턴할형 없음 
//		insertBoard2(Board를 저장한 ArrayList 배열변수)
//		for 출력 메서드 정의 
		
//		insertBoard2() 메서드 호출

	}

}
