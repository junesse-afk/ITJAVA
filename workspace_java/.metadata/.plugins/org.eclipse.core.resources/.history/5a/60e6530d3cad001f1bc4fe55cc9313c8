
public class Test11 {

	public static void main(String[] args) {
		int[] arr = {1, 2, 3};
		System.out.println(arr);
		
		Person2 p = new Person2("홍길동", 20);
		System.out.println(p);
	}
}

class Person2 {
	String name;
	int age;
	public Person(String name, int age) {
		this.name = name;
		this.age = age;
	}
}
