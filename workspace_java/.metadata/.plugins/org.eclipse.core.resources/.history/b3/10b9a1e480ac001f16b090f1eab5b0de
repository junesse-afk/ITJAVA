package test;

public class Student {
//	멤버변수
	private int num;
	public String name;
	public int java;
	public int db;
	public int jsp;
	public int html;
	
//	메서드	
	public int sum() {
		return java + db + jsp + html;
	}
	
	public void intro() {
		System.out.println("번호 : " + num);
		System.out.println("이름 : " + name);
	}
}
