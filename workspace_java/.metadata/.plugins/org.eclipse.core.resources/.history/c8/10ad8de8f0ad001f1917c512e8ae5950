package test;

public class Test1 {
	public static void main(String[] args) {
		
//		static 있으면 객체생성없이 멤버변수, 메서드() 호출
		Student.num = 1000;
//		static 없으면 객체생성없이 호출하면 에러발생
//		Student.name = "홍길동";
		
		Student student = new Student();
		student.num++;
		student.name = "홍길동";
		student.grade = 1;
		
		
		
		
		
	}
}
