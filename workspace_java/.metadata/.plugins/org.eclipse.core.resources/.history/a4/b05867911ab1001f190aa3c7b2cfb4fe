package test;

public class Test2 {

	public static void main(String[] args) {
//		배열 : 같은형의 여러개 기억장소를 정의 사용
		int a[] = new int[3];
		int a1 = 10;
		a[0] = a1;
		a[1] = 20;
		a[2] = 30;
		for(int i = 0; i < a.length; i++) {
			System.out.println(a[i]);
		}
		
		
		String b[] = new String[3];
		String b1 = new String("문자열1");
		b[0] = b1;
		b[1] = "문자열2";
		b[2] = "문자열3";
		
		for(int i = 0; i < b.length; i++) {
			System.out.println(b[i]);
		}
		
		Board c[] = new Board[3];
		
		Board c1 = new Board();
		c1.setName("홍길동");
		c1.setSubject("제목1");
		c1.setContent("내용1");
		
		c[0] = c1;
//		c[0] = new Board();
		
		Board c2 = new Board();
		c2.setName("이길동");
		c2.setSubject("제목2");
		c2.setContent("내용2");
		
		c[1] = c2;
		
		Board c3 = new Board();
		c3.setName("삼길동");
		c3.setSubject("제목3");
		c3.setContent("내용3");
		
		c[2] = c3;
		
		for(int i = 0; i < c.length; i++) {
			System.out.println();
		}
		
		
	}

}
